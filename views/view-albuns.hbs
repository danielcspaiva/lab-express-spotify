<a href="/">Back to Search</a>

<div class='cards'>
    {{#each albums}}
    <div class='card'>
        <img src="{{images.[0].url}}" alt="">
        <div class='card-info'>
            <h1>{{name}}</h1>
            <a href="/albums/tracks/{{id}}">View Tracks</a>
        </div>
    </div>
    {{/each}}
</div>    
